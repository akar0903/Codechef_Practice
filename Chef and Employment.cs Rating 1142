using System;

public class Test
{
	public static void Main()
	{
		// your code goes here
		int t = int.Parse(Console.ReadLine());
		for(int i=0;i<t;i++){
		    int[] arr = Array.ConvertAll(Console.ReadLine().Split(),int.Parse);
		    int N = arr[0];
            int K = arr[1];
            int[] A = Array.ConvertAll(Console.ReadLine().Split(),int.Parse);
            Array.Sort(A);
            Console.WriteLine(A[(N+K)/2]);
		}
	}
}
