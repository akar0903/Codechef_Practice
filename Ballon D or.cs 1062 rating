using System;

public class Test
{
	public static void Main()
	{
		// your code goes here
		int t = int.Parse(Console.ReadLine());
		while(t-->0){
		    int n = int.Parse(Console.ReadLine());
		    int twoCnt = 0;
            string[] numbers = Console.ReadLine().Split();
            for(int i=0;i<n;i++){
                int num = int.Parse(numbers[i]);
                if(num == 2){
                    twoCnt++;
                }
            }
            if (twoCnt % 8 == 0)
                Console.WriteLine("YES");
            else
                Console.WriteLine("NO");
		}
	}
}
